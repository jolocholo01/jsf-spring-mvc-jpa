	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="Sistema de gestao escolar"/>
	<parameter name="idTurma" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT disciplina.descricao AS disciplina_descricao,
	turma.descricao AS turma_descricao,
	classe.descricao AS classe_descricao,
	horario.id_horario_aula,
	horarioaula.aulafinal,
	horarioaula.aulainicial,
	escola.descricao AS escola_descricao,
	horario.id_turma
FROM horario
	INNER JOIN disciplina ON 
	 horario.id_disciplina_segunda = disciplina.id 
	 OR horario.id_disciplina_sexta = disciplina.id 
	 OR horario.id_disciplina_quarta = disciplina.id 
	 OR horario.id_disciplina_terca = disciplina.id 
	 OR horario.id_disciplina_quinta = disciplina.id 
	INNER JOIN turma ON 
	 horario.id_turma = turma.id 
	INNER JOIN classe ON 
	 turma.classe_id = classe.id 
	INNER JOIN horarioaula ON 
	 horario.id_horario_aula = horarioaula.id 
	INNER JOIN escola ON 
	 classe.escola_id = escola.id 
	 AND horarioaula.escola_id = escola.id 
WHERE 
	 horario.id_turma = $P{idTurma}]]>
	</queryString>

	<queryString language="SQL">
		<![CDATA[SELECT usuario.sexo,
	usuario.nome,
	matricula.ano,
	classe.descricao,
	aluno.nome_encarregado,
	escola.descricao As escola_descricao 
FROM matricula
	INNER JOIN aluno ON 
	 matricula.id_aluno = aluno.id 
	INNER JOIN usuario ON 
	 aluno.id = usuario.id 
	INNER JOIN classe ON 
	 matricula.idclasse = classe.id 
	INNER JOIN escola ON 
	 matricula.escola_id = escola.id 
	 AND classe.escola_id = escola.id]]>
	</queryString>
